<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>MetaParkingSystem</display-name>
  <servlet>
    <servlet-name>abc</servlet-name>
    <servlet-class>com.metaparking.EmployeeRegister</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>abc</servlet-name>
    <url-pattern>/employeeRegister</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>pqr</servlet-name>
    <servlet-class>com.metaparking.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>pqr</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>xyz</servlet-name>
    <servlet-class>com.metaparking.VehicleRegister</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>xyz</servlet-name>
    <url-pattern>/vehicleRegister</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>qwe</servlet-name>
    <servlet-class>com.metaparking.UpdateForm</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>qwe</servlet-name>
    <url-pattern>/update</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>poi</servlet-name>
    <servlet-class>com.metaparking.ShowDetails</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>poi</servlet-name>
    <url-pattern>/show</url-pattern>
  </servlet-mapping>
  
</web-app>